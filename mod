      scriptFallback.src = 'https://cdn.shopify.com/s/trekkie.storefront.701f36a47153337658d848c1f5ce233a81e04f59.min.js';
      first.parentNode.insertBefore(scriptFallback, first);
    };
    script.async = true;
    script.src = 'https://cdn.shopify.com/s/trekkie.storefront.701f36a47153337658d848c1f5ce233a81e04f59.min.js';
    first.parentNode.insertBefore(script, first);
  };
